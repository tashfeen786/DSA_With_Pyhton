    "nums = [5, -2, 3, 6, 7, 9,8]\n",
    "n = len(nums)\n",
    "for i in range(n-1):\n",
    "    nums[:] = [nums[-1]] + nums [0: n-1]\n",
    "\n",
    "print(nums)"
   ]
