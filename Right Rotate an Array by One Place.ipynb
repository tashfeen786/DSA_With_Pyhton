   "source": [
    "#Optimal Solution \n",
    "nums = [5, -2, 3, 6, 7, 9,8]\n",
    "temp = nums[n-1]\n",
    "\n",
    "for i in range (n-2, -1, -1):\n",
    "    nums[i+1] = nums[i]\n",
    "nums[0] = temp\n",
    "\n",
    "print(nums)\n"
